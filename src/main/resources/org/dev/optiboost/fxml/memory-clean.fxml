<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>

<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml" fx:controller="org.dev.optiboost.Controller.MemoryUsageController" style="-fx-background-color: #f3f9fe; -fx-background-radius: 0 0 10px 0">
    <Label text="内存使用情况" alignment="CENTER" style="-fx-font-size: 24px;"/>
    <ProgressBar fx:id="progressBar" prefWidth="300.0" progress="0.5"/>
    <Label fx:id="memoryUsageLabel" text="已用内存/总内存：100MB/200MB" alignment="BOTTOM_RIGHT"/>
    <Button text="深度清理" alignment="CENTER" onAction="#goToProcessTable"/>
</VBox>
